{
  "parser": "babel-ts",
  "semi": true,
  "singleQuote": true,
  "bracketSpacing": false,
  "jsxSingleQuote": false,
  "jsxBracketSameLine": false,
  "trailingComma": "es5",
  "tabWidth": 2,
  "printWidth": 80,
  "overrides": [
    {
      "files": "*.scss",
      "options": {
        "parser": "scss"
      }
    }
  ]
}